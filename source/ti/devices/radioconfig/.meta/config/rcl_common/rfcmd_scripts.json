{
  "ble": {
    "CMD_GENERIC_TX": {
      "common.phyFeatures": {
        "#text": "getFeatureSelector(\"subPhy\") | getFeatureSelector(\"codedTxRate\")",
        "role": "update"
      },
      "rfFrequency": {
        "#text": "./conv_ble_channel_to_freq.js,./get_pp_frequency__ble.js,./upd_cf_rf_frequency.js",
        "role": "update",
        "call": "updCfRfFrequency()"
      },
      "txPower.fraction": {
        "#text": "Math.ceil(getTxPowerValue() * 2) & 1",
        "role": "update"
      },
      "txPower.dBm": {
        "#text": "Math.ceil(getTxPowerValue() * 2) >> 1",
        "role": "update"
      }
    },
    "CMD_GENERIC_RX": {
