/*
 *  ======== VCELPF3.Board.c.xdt ========
 */

    let Common          = system.getScript("/ti/drivers/Common.js");
    let VCE             = args[0];
    let instances       = VCE.$instances;
    let inst            = instances[0];
    let operationMode   = inst.operationMode === 'Mirrored' ? 'VCELPF3_OperationMode_MIRRORED' : 'VCELPF3_OperationMode_INTERLEAVED';
%%}
/*
 *  =============================== VCE ===============================
 */

#include <ti/drivers/vce/VCELPF3.h>

/*
 *  ======== VCELPF3_hwAttrs ========
 */

static const VCELPF3_HWAttrs VCELPF3_hwAttrs = {
        .intPriority    = `Common.intPriority2Hex(inst.interruptPriority)`,
        .operationMode  = `operationMode`,
};

void *vceHwAttrs = (void *)&VCELPF3_hwAttrs;
